{
  "compilerOptions": {
    "target": "ESNext",
    "module": "module",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "declaration": true // 生成 .d.ts 类型声明文件
  },
  "include": ["src"]
}
